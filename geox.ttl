  skos:historyNote "This class is taken as-is from the QUDT ontology."@en ;
  skos:prefLabel "Area" ;
.
qk:Distance
  a qudt:QuantityKind ;
  rdfs:label "Distance" ;
  skos:prefLabel "Distance" ;
.
qk:Length
  a qudt:QuantityKind ;
  rdfs:label "Length" ;
  skos:exactMatch <http://dbpedia.org/resource/Length> ;
  skos:prefLabel "Length" ;
