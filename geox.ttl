.
qk:Volume
  a qudt:QuantityKind ;
  rdfs:label "Volume" ;
  skos:exactMatch <http://dbpedia.org/resource/Volume> ;
  skos:prefLabel "Volume" ;
.
geo:SpatialObject
  a owl:Class ;
.
geo:sfContains
  owl:inverseOf geo:sfWithin ;
.
geo:sfOverlaps
  a owl:IrreflexiveProperty ;
.
geo:sfWithin
  owl:inverseOf geo:sfContains ;
.
rdfs:prefLabel
  a owl:AnnotationProperty ;
.
skos:usageNote
